class DivisorPattern{
  public static void main(String[] args){
    int n = Integer.parseInt();
    for(int i = 1; i <= n; i++) {
      for(int j = 1; j <= n; j++) {
        if( (i % j == 0) || (j % i == 0)){\
          System.out.printf("*");
        else System.out.print(" ");
        }
        System.out.println(i);
      }
    }
  }

}